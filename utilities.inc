DELAY_DEBOUNCE:
DELAY5MS:			;@11.0592MHz
	NOP
	NOP
	PUSH 30H
	PUSH 31H
	MOV 30H,#44
	MOV 31H,#2
NEXT_DEBOUNCE:
	DJNZ 31H,NEXT_DEBOUNCE
	DJNZ 30H,NEXT_DEBOUNCE
	POP 31H
	POP 30H
	RET

